{
  "manifest_version": 3,
  "name": "Bookmarks & Tabs Search",
  "version": "0.0.5",
  "description": "Ctrl+M\nThis extension displays list of the opened tabs (except the incognito) and all bookmarks to quickly find and open them.",
  "author": "UnnamedXAer",
  "minimum_chrome_version": "110",
  "permissions": ["activeTab", "bookmarks", "tabs", "tabGroups"],
  "background": {
    "service_worker": "src/service-worker.js"
  },
  "action": {
    "default_title": "Search Bookmark",
    "default_popup": "src/popup/popup.html"
  },
  "commands": {
    "_execute_action": {
      "description": "Search Tabs & Bookmarks",
      "suggested_key": { "default": "Ctrl+M" }
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
